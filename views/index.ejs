<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Login INDO BETS</title>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<link rel="icon" type="image/png" href="/login/images/icons/favicon.ico" />
		<link
			rel="stylesheet"
			type="text/css"
			href="/login/vendor/bootstrap/css/bootstrap.min.css"
		/>
		<link
			rel="stylesheet"
			type="text/css"
			href="/login/fonts/font-awesome-4.7.0/css/font-awesome.min.css"
		/>
		<link
			rel="stylesheet"
			type="text/css"
			href="/login/fonts/iconic/css/material-design-iconic-font.min.css"
		/>
		<link
			rel="stylesheet"
			type="text/css"
			href="/login/vendor/animate/animate.css"
		/>
		<link
			rel="stylesheet"
			type="text/css"
			href="/login/vendor/css-hamburgers/hamburgers.min.css"
		/>
		<link
			rel="stylesheet"
			type="text/css"
			href="/login/vendor/animsition/css/animsition.min.css"
		/>
		<link rel="stylesheet" type="text/css" href="/login/css/util.css" />
		<link rel="stylesheet" type="text/css" href="/login/css/main.css" />
	</head>

	<style type="text/css">
		canvas {
			display: block;
			/* vertical-align: bottom;  */
		}
		.center {
			color: aliceblue;
			text-align: center;
		}
	</style>
	<body>
		<div class="limiter">
			<div class="container-login100">
				<div class="wrap-login100 p-t-85 p-b-20">
					<form
						action="/register/loginDashboard"
						method="POST"
						class="login100-form validate-form"
					>
						<span class="login100-form-avatar">
							<img
								src="/login/images/app_new_logo.png"
								alt="logo"
								width="200"
								height="150"
							/>
						</span>
						<div
							class="wrap-input100 validate-input m-t-85 m-b-35"
							data-validate="Enter username"
						>
							<input
								class="input100"
								type="text"
								name="user_username"
								autocomplete="off"
							/>
							<span class="focus-input100" data-placeholder="Username"></span>
						</div>
						<div
							class="wrap-input100 validate-input m-b-50"
							data-validate="Enter password"
						>
							<input
								class="input100"
								type="password"
								name="user_password"
								autocomplete="off"
							/>
							<span class="focus-input100" data-placeholder="Password"></span>
						</div>
						<div class="container-login100-form-btn">
							<input type="submit" class="login100-form-btn" value="Login" />
						</div>
						<br />
					</form>
					<span class="center"><% if(message) {%> <%= message %> <% } %></span>
				</div>
			</div>
		</div>
		<div
			style="position: absolute; top: 0px; *left: 0px; cursor: pointer; opacity: 0.9; z-index: -1 !important;"
		></div>
		<script src="/login/vendor/jquery/jquery-3.2.1.min.js"></script>
		<script src="/login/vendor/animsition/js/animsition.min.js"></script>
		<script src="/login/vendor/bootstrap/js/popper.js"></script>
		<script src="/login/vendor/bootstrap/js/bootstrap.min.js"></script>
		<script src="/login/js/main.js"></script>
	</body>
</html>
