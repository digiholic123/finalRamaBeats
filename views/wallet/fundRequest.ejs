<script>
   
    function  getCurrentBal(obj)
    {
        const rowId = $(obj).attr('data-rowId');
        const id = $(obj).attr('data-userId');
        const amt = $(obj).attr('data-amount');

        $.ajax({
            type: "POST",
            url: "/fundRequest/getBal",
            data: {id : id},
            success: function(data) 
            { 
                $("#mySmallModalLabel").html(data.username);
                $("#balancecURRENT").html('Current Bal : '+ data.wallet_balance+'/-');
                $("#userid").val(id);
                $("#rowId").val(rowId);
                $("#amount").val(amt);
                $("#username").val(data.username);
            }
        });
    }

    function pendingDebit()
    {
        $("#datatable").DataTable().destroy();
        $.ajax({
            type: "get",
            url: "/fundRequest/other",
            data: { data : "pendingDebit"},
            contentType: "text/plain",
            dataType: 'json',
            success: function (data) {
                if (Object.keys(data).length === 0)
                {
                    $('#datatable').DataTable().clear().draw();
                    toastr["warning"]("No Pending Debit Request Found","Indo Bets Games");
                }
                else
                {
                    let html = '';
                    let i = 1;
                    let total = 0;
                    data.forEach(function(e) {
                        total += e.reqAmount;
                        html += '<tr id="dhan'+ e.userId +'"><td>'+i +'</td><td>'+ e.fullname +'</td>\
                                <td>'+ e.username +'</td>\
                                <td>'+ e.mobile +'</td>\
                                <td><p class="badge badge-purple">' + e.reqAmount + '</p></td>\
                                <td><p class="badge badge-purple">' + e.reqType + '</p></td>\
                                <td><p class="badge badge-purple">' + e.withdrawalMode + '</p></td>\
                                <td>'+ e.reqStatus +'</td>\
                                <td>'+ e.reqDate +" " + e.reqTime +'</td>\
                                <td><button data-username="' + e.username + '" data-userId="'+ e.userId +'" data-rowId="'+ e._id +'" data-amount="'+ e.reqAmount +'" type="button" class="btn btn-dark waves-effect waves-light btn-xs mr-1"   data-toggle="modal" data-target=".bs-example-modal-sm" onclick="getCurrentBal(this)" title="Approve Debit Request"><i class="fas fa-check-circle"></i></button><button data-username="' + e.username + '" data-userId="'+ e.userId +'" data-rowId="'+ e._id +'" data-stat="2" type="button" class="btn waves-effect waves-light btn-danger btn-xs mr-1" id="creditDecline" onclick="decline(this)" title="Decline Request"><i class="fas fa-trash"></i></button><button data-username="' + e.username + '" data-userId="'+ e.userId +'" type="button" class="btn waves-effect waves-light btn-purple btn-xs mr-1" id="userProfile" data-toggle="modal" data-target="#custom-modal-edit" onclick="getProfile(this)" title="User Profile"><i class="fas fa-user"></i></button></td></tr>';
                                i++
                    });
                    $("#debitBody").html(html);
                    $("#datatable").dataTable();
                    document.getElementById("totAmount").innerHTML = "Total Amount : " + total + "/-";
                    $('#datatable5').DataTable();
                }
            },
            error: function (e) {
                alert("error: " + JSON.stringify(e));
            }
        });
    }

