<% layout('../layout') %>
<style>
	table.dataTable {
		border-collapse: collapse !important;
		margin-bottom: 15px !important;
		width: 45% !important;
		margin: 0 auto;
	}
	#td_6{
		display:block
	}
</style>
<div class="container-fluid">
	<div class="row">
		<div class="col-xl-12 col-md-12">
			<div class="card-box pt-0 pb-0">
				<nav aria-label="breadcrumb">
					<ol class="breadcrumb">
						<li class="breadcrumb-item" aria-current="page">
							<a href="/"> Home </a>
						</li>
						<li class="breadcrumb-item active" aria-current="page">
							Final OC Cutting Group
						</li>
					</ol>
				</nav>
			</div>
		</div>
	</div>
</div>
<div class="container-fluid">
	<div class="row">
		<div class="col-xl-12 col-md-12">
			<div class="card-box">
				<h4 class="mt-0 mb-0 header-title text-center">
					Final OC Cutting Group
				</h4>
			</div>
		</div>
	</div>
</div>
<div class="container-fluid">
	<div class="row">
		<div class="col-xl-8 col-md-12">
			<div class="card-box">
				<p>Search Panel</p>
				<div class="form-row">
					<div class="form-group col-xl-3">
						<label for="gamename">Date</label>
						<input
							type="text"
							class="datepicker form-control"
							autocomplete="off"
							id="bidDate"
						/>
					</div>
					<div class="form-group col-xl-3">
						<label for="gamename">Provider Name</label>
						<select class="form-control" id="providerID">
							<% for(index in data){ %>
							<option
								value="<%= data[index]._id %>|||<%= data[index].providerName %>"
								><%= data[index].providerName%></option
							>
							<%} %>
						</select>
					</div>
					<div class="form-group col-xl-3">
						<label for="gamename">Game Session</label>
						<select name="session" class="form-control" id="gameSession">
							<option value="Open">Open</option>
							<option value="Close">Close</option>
						</select>
					</div>
					<div class="form-group col-xl-3">
						<label for="gamename">Divide By</label>
						<input
							type="text"
							class="form-control"
							autocomplete="off"
							id="divideBy"
							value="16"
						/>
					</div>
					<div class="col-xl-12">
						<button
							type="submit"
							class="btn btn-primary btn-sm"
							onclick="finaOC()"
							style="float: right;"
						>
							Submit
						</button>
					</div>
				</div>
			</div>
		</div>
		<div class="col-xl-4 col-md-12">
			<div class="card-box">
				<div class="table-responsive">
					<table class="table table-bordered mb-0">
						<thead class="thead-light">
							<tr>
								<th>#</th>
								<th>Type</th>
								<th>Amount</th>
							</tr>
						</thead>
						<tbody id="totalCal"></tbody>
					</table>
				</div>
			</div>
		</div>
	</div>
	<div class="container-fluid" id="signleDigitDiv" style="display: none;">
		<div class="row">
			<div class="col-xl-12 col-md-12">
				<div class="card-box">
					<h3>Single Digits</h3>
					<div class="col-xl-6 col-md-6">
						<div class="table-responsive" data-pattern="priority-columns">
							<table
								id="datatable3"
								class="newWidth table table-bordered dt-responsive nowrap text-dark"
							>
								<thead>
									<tr class="newWidth">
										<th>Digits</th>
										<th>Cutting</th>
										<th>Total</th>
									</tr>
									<tr>
										<th id="single" colspan="3"></th>
									</tr>
								</thead>
								<tbody class="newWidth" id="singleDigit"></tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="container-fluid" id="panaDiv" style="display: none;">
		<div class="row">
			<div class="col-xl-12 col-md-12">
				<div class="card-box">
					<h3>Panna Bids</h3>
					<div class="col-xl-6 col-md-6">
						<div class="table-responsive" data-pattern="priority-columns">
							<table
								id="datatable4"
								class="table table-bordered dt-responsive nowrap text-dark"
							>
								<thead>
									<tr>
										<th>Digits</th>
										<th>Cutting</th>
										<th>Total</th>
									</tr>
									<tr>
										<th id="mingle" colspan="3"></th>
									</tr>
								</thead>
								<tbody id="panaTab"></tbody>
								<tfoot id="sumTotal"></tfoot>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
